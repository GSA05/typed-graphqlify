{
  "name": "typed-graphqlify",
  "version": "2.0.5-alpha",
  "description": "JavaScript class that render GraphQL. Better TypeScript + GraphQL experience.",
  "keywords": [
    "graphql",
    "typescript",
    "utility"
  ],
  "repository": "git@github.com:acro5piano/typed-graphqlify",
  "author": "gosho-kazuya <ketsume0211@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "jsnext:main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist/*",
    "dist": "npm-run-all clean build",
    "format": "prettier --write src/*.ts src/**/*.ts",
    "lint": "tslint --fix src/*.ts src/**/*.ts",
    "test": "jest --maxWorkers=4",
    "test:coverage": "jest --maxWorkers=4 --coverage",
    "prepublish": "npm run dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/webpack-env": "^1.13.6",
    "codecov": "^3.1.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rollup": "^0.68.2",
    "rollup-plugin-typescript2": "^0.18.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  }
}
